{
  "manifest_version": 3,
  "name": "Delta Guestbook Extractor",
  "version": "2.0",
  "description": "Extract unique domains, filter Web 2.0s, and exclude domains from an uploaded Excel exclusion list.",
  "permissions": ["scripting", "tabs", "activeTab"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}